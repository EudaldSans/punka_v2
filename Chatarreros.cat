                  <characteristics>
                    <characteristic name="Efecto" typeId="615a-5192-19d4-fe3c">Podría haber competido en salto de longitud. A la hora de Saltar, se considerará que su peana es el doble de grande (50 mm).</characteristic>
                  </characteristics>
                </profile>
              </profiles>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Navajero" hidden="false" id="9fbd-f70-91dd-ac9">
              <costs>
                <cost name="Puntos" typeId="e515-0c5b-47aa-1328" value="5"/>
