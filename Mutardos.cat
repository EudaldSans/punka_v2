        <profile name="Kim y Fostiador" typeId="65f8-965a-8432-c96b" typeName="Miniatura" hidden="false" id="6a19-3038-7dc-8f0f">
          <characteristics>
            <characteristic name="Acciones" typeId="d05b-aff7-3ed3-1bb6">3</characteristic>
            <characteristic name="Combate" typeId="6e27-b2ad-21d6-48f8">6 (5)</characteristic>
