                <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="4192-46a0-b567-e0e7-max" includeChildSelections="false"/>
              </constraints>
            </selectionEntry>
            <selectionEntry type="upgrade" import="true" name="Invocar al pantano" hidden="false" id="d858-ba1c-1ef3-e576">
